  vertical-align: middle;
  margin-right: 10rpx;
  margin-top: 6rpx;
}
.goods-list-box .price_ident,
.goods-list-box .price_number,
.goods-list-box .price_mantissa {
  display: inline-block;
  vertical-align: bottom;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #FF3366;
}
.goods-list-box .price_ident {
