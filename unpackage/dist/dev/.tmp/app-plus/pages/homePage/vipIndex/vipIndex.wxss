.r-info-box {
  position: relative;
  font-size: 28rpx;
  width: 440rpx;
  height: 240rpx;
}
.r-info-box .r-title {
  width: 418rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #2F3A40;
  margin-top: 10rpx;
  display: block;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  -o-text-overflow: ellipsis;
     text-overflow: ellipsis;
  word-break: break-all;
  white-space: normal;
}
.r-info-box .r-described {
  margin-top: 6rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #B2B2B2;
}
.r-info-box .r-described .text {
  width: auto;
}
.r-info-box .r-described .rateRange {
  margin-right: 0rpx;
}
.r-info-box .r-described .salesCount {
  margin-left: 60rpx;
}
.r-info-box .r-foot {
  bottom: 0px;
  position: absolute;
  font-size: 24rpx;
  width: 100%;
}
.shoppingBuy {
  position: absolute;
  cursor: pointer;
  width: 44rpx;
  height: 44rpx;
  right: 24rpx;
  bottom: 30rpx;
  z-index: 2;
}

.loading {
  position: absolute;
  width: 100%;
  height: 100%;
  background: #fff;
  top: 0;
  bottom: 0;
  overflow: hidden;
  z-index: 100;
}
.loading .loadCon .imgText {
  text-align: center;
  width: 180rpx;
  height: 150rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  left: 0;
  margin: auto;
}
.loading .loadCon ._img {
  width: 80rpx;
  height: 80rpx;
  /*animation (动画) :绑定选择器, 4s完成动画 linear(匀速) infinite(循环) */
  -webkit-animation: que 2s linear infinite;
          animation: que 2s linear infinite;
}
.loading .loadCon .text {
  color: #666;
  font-size: 30rpx;
  margin-left: 50rpx;
  margin-top: 10rpx;
}
@-webkit-keyframes que {
  /*以百分比来规定改变发生的时间 也可以通过"from"和"to",等价于0% 和 100%*/
0% {
    /*rotate(2D旋转) scale(放大或者缩小) translate(移动) skew(翻转)*/
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
@keyframes que {
  /*以百分比来规定改变发生的时间 也可以通过"from"和"to",等价于0% 和 100%*/
0% {
    /*rotate(2D旋转) scale(放大或者缩小) translate(移动) skew(翻转)*/
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}

