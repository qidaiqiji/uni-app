uni-page-body {
  background: #fff;
}
.search-page {
  background: #fff;
  overflow: hidden;
}
.searchPart {
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  width: 702rpx;
  margin: auto;
  margin-bottom: 40rpx;
}
.searchPart .search {
  width: 600rpx;
}
.searchPart .search-btn {
  text-align: center;
  width: 100rpx;
  height: 60rpx;
  line-height: 60rpx;
  padding: 14rpx 0;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: #ff3366;
}
.box {
  width: 702rpx;
  margin: auto;
}
.box .title {
  position: relative;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: #2f3a40;
  line-height: 48rpx;
}
.box .iconDel {
  display: block;
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  right: 0;
  top: 50%;
  margin-top: -20rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: center;
  background-size: 30rpx 30rpx;
}
.box .search-tag {
  margin-top: 30rpx;
  min-height: 160rpx;
  margin-bottom: 20rpx;
}
.box .tag-item {
  display: inline-block;
  margin-right: 16rpx;
  margin-bottom: 16rpx;
  background: #f3f3f3;
  -webkit-border-radius: 4rpx;
          border-radius: 4rpx;
  padding: 13rpx 16rpx;
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: #2f3a40;
  line-height: 30rpx;
}
.box .tag-item .name {
  font-size: 24rpx;
}
.box .tag-item .icon {
  width: 30rpx;
  height: 30rpx;
  display: inline-block;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: center;
  background-size: 30rpx 30rpx;
}

