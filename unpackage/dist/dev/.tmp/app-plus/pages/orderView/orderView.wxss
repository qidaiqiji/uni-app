.ibt {
  display: inline-block;
  vertical-align: top;
}
.o-border {
  border-top: 1px solid #e5e5e5;
  border-bottom: 1px solid #e5e5e5;
  padding: 0 24rpx;
  background: #ffffff;
  margin-bottom: 12rpx;
}
.o-header {
  padding: 0 24rpx;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 1px solid #e5e5e5;
  font-size: 28rpx;
  font-family: MicrosoftYaHei;
  font-weight: 400;
  color: #2f3a40;
  background: #ffffff;
  margin-bottom: 12rpx;
}
.o-header .sn {
  color: #889696;
}
.o-addressInfo {
  position: relative;
  overflow: hidden;
}
.o-addressInfo .img {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  left: 0;
  top: 0;
  background-image: url(data:image/png;base64,);
  background-position: left top;
  background-repeat: no-repeat;
  background-size: 40rpx 40rpx;
}
.o-addressInfo .info {
  position: relative;
  padding-left: 44rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: #2f3a40;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}
.o-addressInfo .name,
.o-addressInfo .phone {
  display: inline-block;
}
.o-addressInfo .phone {
  margin-left: 20rpx;
}
.o-addressInfo .address {
  word-spacing: normal;
  word-break: break-all;
  white-space: normal;
  margin-top: 10rpx;
}
.o-shippingInfo {
  font-size: 24rpx;
  font-family: MicrosoftYaHei;
  font-weight: 400;
  color: #889696;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}
.o-shippingInfo .item {
  margin-bottom: 20rpx;
}
.o-shippingInfo .label {
  width: 200rpx;
  display: inline-block;
}
.o-shippingInfo .value {
  color: #2f3a40;
}
.o-shippingInfo .pay-value {
  color: #ff3867;
}
.o-feeInfo {
  background: #cde6ff;
  font-size: 24rpx;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}
.o-feeInfo .tips {
  font-size: 24rpx;
  font-family: MicrosoftYaHei-Bold;
  font-weight: bold;
  color: #ff3366;
  margin-bottom: 24rpx;
}
.o-feeInfo .zhifa {
  color: #2f3a40;
}
.o-feeInfo .zhifa .i {
  color: #ff3366;
}
.o-feeInfo .daifa {
  color: #2f3a40;
}
.o-feeInfo .daifa .i {
  color: #ff3366;
}
.o-subOrderList {
  padding-bottom: 0rpx;
}
.o-subOrderList .head {
  position: relative;
  padding: 0 24rpx;
  height: 88rpx;
  line-height: 88rpx;
  background: #ecf1f5;
  font-size: 28rpx;
}
.o-subOrderList .head::after {
  content: ' ';
  width: 702rpx;
  height: 1px;
  background: #e9f3f5;
  position: absolute;
  bottom: 0;
  left: 24rpx;
}
.o-subOrderList .head .text {
  font-family: MicrosoftYaHei;
  font-weight: 400;
  color: #5aaefc;
}
.o-subOrderList .body {
  background: #ffffff;
}
.o-subOrderList .body .item {
  padding: 24rpx 0;
  position: relative;
  width: 702rpx;
  margin: auto;
}
.o-subOrderList .body .item::after {
  content: ' ';
  width: 702rpx;
  height: 1px;
  background: #e9f3f5;
  position: absolute;
  bottom: 0;
  left: 0;
}
.o-subOrderList .body .img-box {
  margin-right: 20rpx;
  display: inline-block;
  position: relative;
  overflow: hidden;
  width: 110rpx;
  height: 110rpx;
  text-align: center;
  background: #ffffff;
  border: 1px solid #e9f3f5;
  -webkit-border-radius: 6rpx;
          border-radius: 6rpx;
}
.o-subOrderList .body .img-box .img {
  margin: auto;
  max-width: 100%;
  max-height: 100%;
}
.o-subOrderList .body .img-box .text {
  width: 100%;
  position: absolute;
  bottom: 0;
  left: 0;
  text-align: center;
  padding: 4rpx 0;
  color: #ffffff;
  font-size: 20rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
}
.o-subOrderList .body .isWuliao {
  border: 1px solid #53a7fd;
}
.o-subOrderList .body .isWuliao .text {
  background: rgba(83, 167, 253, 0.8);
}
.o-subOrderList .body .isGift {
  border: 1px solid #ff3366;
}
.o-subOrderList .body .isGift .text {
  background: rgba(255, 79, 123, 0.8);
}
.o-subOrderList .body .info-box {
  display: inline-block;
  position: relative;
  width: 400rpx;
  height: 110rpx;
  font-size: 28rpx;
  font-family: MicrosoftYaHei;
  font-weight: 400;
  color: #333333;
}
.o-subOrderList .body .title {
  color: #333333;
  word-break: break-all;
  word-spacing: normal;
  white-space: nowrap;
  -o-text-overflow: ellipsis;
     text-overflow: ellipsis;
  overflow: hidden;
}
.o-subOrderList .body .foot {
  font-size: 28rpx;
  position: absolute;
  bottom: 0;
  left: 0;
}
.o-subOrderList .body .f-price {
  color: #333333;
}
.o-subOrderList .body .f-text {
  margin-left: 20rpx;
  font-size: 24rpx;
  color: #999999;
}
.foot-count {
  padding: 0 24rpx;
  overflow: hidden;
  background: #fff;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 28rpx;
  font-family: MicrosoftYaHei;
  font-weight: 400;
  color: #889696;
}
.foot-count .f-price {
  color: #2f3a40;
}
.o-page-foot-empty {
  width: 100%;
  height: 98rpx;
}
.o-page-foot {
  width: 100%;
  height: 98rpx;
  background: #ffffff;
  position: fixed;
  bottom: 0;
}
.o-page-foot .fr {
  margin-top: 22rpx;
  padding-right: 24rpx;
}
.order-detail {
  overflow: hidden;
}

