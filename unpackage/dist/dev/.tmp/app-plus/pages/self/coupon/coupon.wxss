.coupon-warp {
  position: relative;
  overflow: hidden;
  background: #F3F3F3;
  width: 100%;
  min-height: 981rpx;
}
.coupon-warp__top_img {
  width: 100%;
  height: 240rpx;
  vertical-align: middle;
}
.coupon-warp__select {
  margin: 24rpx 0;
  padding: 0 24rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  background: #F3F3F3;
}
.coupon-warp__select_item {
  width: 110rpx;
  height: 48rpx;
  line-height: 48rpx;
  -webkit-border-radius: 24rpx;
          border-radius: 24rpx;
  text-align: center;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: #2f3a40;
  margin-bottom: 10rpx;
  display: inline-block;
  margin-right: 20rpx;
  cursor: pointer;
}
.coupon-warp__select_item.active {
  background: #ff3366;
  color: #ffffff;
}
.isFixed {
  position: fixed;
  width: 100%;
  z-index: 5;
  padding-top: 10rpx;
  margin: 0;
  top: 0px;
}
.emptyview {
  height: 170rpx;
  visibility: hidden;
}

